{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "",
    "outDir": "dist",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
